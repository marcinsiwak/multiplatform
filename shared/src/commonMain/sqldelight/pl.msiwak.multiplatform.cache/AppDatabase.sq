import kotlin.collections.List;

CREATE TABLE Summary (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    exerciseType    TEXT NOT NULL,
    results     TEXT AS List<String>    NOT NULL
);

insertSummary:
INSERT INTO Summary(exerciseType, results)
VALUES (?,?);

removeAllSummaries:
DELETE FROM Summary;

selectFromSummary:
SELECT * FROM Summary
WHERE id = ?;

selectAllFromSummary:
SELECT * FROM Summary;